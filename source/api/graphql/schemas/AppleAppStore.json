{
    "types": {
        "input": {
            "PaymentMethodType": {
                "kind": "enum",
                "type": "PaymentMethodType",
                "values": ["CreditCard", "AppleInAppPurchase"]
            },
            "PaymentProcessorType": {
                "kind": "enum",
                "type": "PaymentProcessorType",
                "values": ["StripeProxy", "StripeEmbedded", "AppleInAppPurchase"]
            },
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "CreditCardType": {
                "kind": "enum",
                "type": "CreditCardType",
                "values": ["Visa", "Mastercard", "Amex", "Discover", "Unknown"]
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "CommerceOrderStatus": {
                "kind": "enum",
                "type": "CommerceOrderStatus",
                "description": "The status of the order",
                "values": [
                    "WaitPayment",
                    "Pending",
                    "FailToConfirm",
                    "Open",
                    "Complete",
                    "Archived",
                    "Cancelled",
                    "OutOfStock"
                ]
            },
            "PaymentStatus": {
                "kind": "enum",
                "type": "PaymentStatus",
                "description": "The status of the payment",
                "values": ["Pending", "Confirmed", "Authorized", "Captured", "FailToAuthorize", "Cancelled"]
            },
            "CommerceOrderFulfillmentStatus": {
                "kind": "enum",
                "type": "CommerceOrderFulfillmentStatus",
                "description": "The fulfillment status of the order",
                "values": ["NotStart", "Unfulfilled", "PartiallyFulfilled", "Fulfilled", "Shipped", "Cancelled"]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "MonetaryDecimal": {
                "kind": "scalar",
                "type": "MonetaryDecimal",
                "description": "Monetary decimal custom scalar type, we stored and operate the value in cents, and this scalar will convert the value to dollar when read and convert the value to cents when write."
            },
            "JSON": {
                "kind": "scalar",
                "type": "JSON",
                "description": "The `JSON` scalar type represents JSON values as specified by [ECMA-404](http://www.ecma-international.org/publications/files/ECMA-ST/ECMA-404.pdf)."
            },
            "CommerceOrderLineItemStatus": {
                "kind": "enum",
                "type": "CommerceOrderLineItemStatus",
                "description": "The status of the order line item",
                "values": ["Pending", "Shipped", "Cancelled"]
            },
            "CommerceOrderLogVisibility": {
                "kind": "enum",
                "type": "CommerceOrderLogVisibility",
                "description": "The visibility of the order log.",
                "values": ["Public", "Internal"]
            },
            "CommerceOrderLogSource": {
                "kind": "enum",
                "type": "CommerceOrderLogSource",
                "description": "The source of the order log.",
                "values": ["System", "User", "CustomerSupport"]
            },
            "DiscountType": {
                "kind": "enum",
                "type": "DiscountType",
                "description": "The type of the discount.",
                "values": ["Automatic", "Code"]
            },
            "DiscountAllocationTarget": {
                "kind": "enum",
                "type": "DiscountAllocationTarget",
                "values": ["Each", "Across", "ShippingAmount", "ShippingBreakdown"]
            },
            "Float": {
                "kind": "scalar",
                "type": "Float",
                "description": "The `Float` scalar type represents signed double-precision fractional values as specified by [IEEE 754](https://en.wikipedia.org/wiki/IEEE_floating_point)."
            },
            "DiscountValueType": {
                "kind": "enum",
                "type": "DiscountValueType",
                "values": ["FixedAmount", "Percentage"]
            },
            "DiscountAllocationMethod": {
                "kind": "enum",
                "type": "DiscountAllocationMethod",
                "values": ["Flat", "BuyXGetY", "BuyXGetFollowing"]
            },
            "DiscountConditionType": {
                "kind": "enum",
                "type": "DiscountConditionType",
                "values": ["Vendors", "Products", "ProductVariants"]
            },
            "DiscountConditionTarget": {
                "kind": "enum",
                "type": "DiscountConditionTarget",
                "values": ["EntireOrder", "LineItem"]
            },
            "ShipmentStatus": {
                "kind": "enum",
                "type": "ShipmentStatus",
                "description": "The status of the shipping",
                "values": ["Pending", "LabelPurchased", "LabelPrinted", "Shipped", "Delivered", "Cancelled"]
            },
            "DeliveryStatus": {
                "kind": "enum",
                "type": "DeliveryStatus",
                "description": "The status of the delivery",
                "values": ["AttemptedDelivery", "ReadyForPickup", "InTransit", "OutForDelivery", "Delivered"]
            },
            "ShippingLabelSource": {
                "kind": "enum",
                "type": "ShippingLabelSource",
                "values": ["Stamps", "Others"]
            },
            "ShippingServiceType": {
                "kind": "enum",
                "type": "ShippingServiceType",
                "values": [
                    "USPSFirstClassMail",
                    "USPSPriorityMail",
                    "USPSPriorityMailExpress",
                    "USPSParcelSelect",
                    "USPSMediaMail"
                ]
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": ["SCALAR", "OBJECT", "INTERFACE", "UNION", "ENUM", "INPUT_OBJECT", "LIST", "NON_NULL"]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}
