{
    "types": {
        "input": {
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "Float": {
                "kind": "scalar",
                "type": "Float",
                "description": "The `Float` scalar type represents signed double-precision fractional values as specified by [IEEE 754](https://en.wikipedia.org/wiki/IEEE_floating_point)."
            },
            "PaginationInput": {
                "kind": "object",
                "type": "PaginationInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    },
                    {
                        "kind": "object",
                        "type": "ColumnFilterInput",
                        "required": true,
                        "name": "filters"
                    },
                    {
                        "kind": "object",
                        "type": "OrderByInput",
                        "required": true,
                        "name": "orderBy"
                    }
                ]
            },
            "ColumnFilterInput": {
                "kind": "object",
                "type": "ColumnFilterInput",
                "fields": [
                    {
                        "kind": "enum",
                        "type": "ColumnFilterConditionOperator",
                        "required": true,
                        "name": "operator"
                    },
                    {
                        "kind": "scalar",
                        "type": "Boolean",
                        "required": false,
                        "name": "caseSensitive"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "column"
                    },
                    {
                        "kind": "scalar",
                        "type": "JSON",
                        "required": true,
                        "name": "value"
                    }
                ]
            },
            "ColumnFilterConditionOperator": {
                "kind": "enum",
                "type": "ColumnFilterConditionOperator",
                "description": "The operator of a field filter",
                "values": [
                    "Equal",
                    "NotEqual",
                    "GreaterThan",
                    "GreaterThanOrEqual",
                    "LessThan",
                    "LessThanOrEqual",
                    "Like",
                    "NotLike",
                    "In",
                    "NotIn",
                    "IsNull",
                    "IsNotNull"
                ]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "JSON": {
                "kind": "scalar",
                "type": "JSON",
                "description": "The `JSON` scalar type represents JSON values as specified by [ECMA-404](http://www.ecma-international.org/publications/files/ECMA-ST/ECMA-404.pdf)."
            },
            "OrderByInput": {
                "kind": "object",
                "type": "OrderByInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "key"
                    },
                    {
                        "kind": "enum",
                        "type": "OrderByDirection",
                        "required": false,
                        "name": "direction"
                    }
                ]
            },
            "OrderByDirection": {
                "kind": "enum",
                "type": "OrderByDirection",
                "description": "The order direction of a query",
                "values": ["Ascending", "Descending"]
            },
            "UpdateAssetMetadataInput": {
                "kind": "object",
                "type": "UpdateAssetMetadataInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "assetId"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "name"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "altText"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "tags"
                    }
                ]
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": ["SCALAR", "OBJECT", "INTERFACE", "UNION", "ENUM", "INPUT_OBJECT", "LIST", "NON_NULL"]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}
