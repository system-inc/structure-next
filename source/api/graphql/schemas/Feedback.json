{
    "types": {
        "input": {
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "RichContentFormat": {
                "kind": "enum",
                "type": "RichContentFormat",
                "description": "The format of the string rich-content",
                "values": ["Markdown", "Html", "PlainText"]
            },
            "PaginationInput": {
                "kind": "object",
                "type": "PaginationInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    },
                    {
                        "kind": "object",
                        "type": "ColumnFilterInput",
                        "required": true,
                        "name": "filters"
                    },
                    {
                        "kind": "object",
                        "type": "OrderByInput",
                        "required": true,
                        "name": "orderBy"
                    }
                ]
            },
            "ColumnFilterInput": {
                "kind": "object",
                "type": "ColumnFilterInput",
                "fields": [
                    {
                        "kind": "enum",
                        "type": "ColumnFilterConditionOperator",
                        "required": true,
                        "name": "operator"
                    },
                    {
                        "kind": "scalar",
                        "type": "Boolean",
                        "required": false,
                        "name": "caseSensitive"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "column"
                    },
                    {
                        "kind": "scalar",
                        "type": "JSON",
                        "required": true,
                        "name": "value"
                    }
                ]
            },
            "ColumnFilterConditionOperator": {
                "kind": "enum",
                "type": "ColumnFilterConditionOperator",
                "description": "The operator of a field filter",
                "values": [
                    "Equal",
                    "NotEqual",
                    "GreaterThan",
                    "GreaterThanOrEqual",
                    "LessThan",
                    "LessThanOrEqual",
                    "Like",
                    "NotLike",
                    "In",
                    "NotIn",
                    "IsNull",
                    "IsNotNull"
                ]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "JSON": {
                "kind": "scalar",
                "type": "JSON",
                "description": "The `JSON` scalar type represents JSON values as specified by [ECMA-404](http://www.ecma-international.org/publications/files/ECMA-ST/ECMA-404.pdf)."
            },
            "OrderByInput": {
                "kind": "object",
                "type": "OrderByInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "key"
                    },
                    {
                        "kind": "enum",
                        "type": "OrderByDirection",
                        "required": false,
                        "name": "direction"
                    }
                ]
            },
            "OrderByDirection": {
                "kind": "enum",
                "type": "OrderByDirection",
                "description": "The order direction of a query",
                "values": ["Ascending", "Descending"]
            },
            "CreateFeedbackInput": {
                "kind": "object",
                "type": "CreateFeedbackInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "subject"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "identifier"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "reaction"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "content"
                    },
                    {
                        "kind": "enum",
                        "type": "RichContentFormat",
                        "required": false,
                        "name": "contentType"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "emailAddress"
                    }
                ]
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": ["SCALAR", "OBJECT", "INTERFACE", "UNION", "ENUM", "INPUT_OBJECT", "LIST", "NON_NULL"]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}
